 ul.tag-label li {
     position: absolute;
     top: 0;
     left: auto;
     right: 0;
     text-align: center;
     background-color: #0F0F0F;
     color: white;
     {% comment %} height: 30px; {% endcomment %}
     padding: 8px 15px;
     font-size: 12px;
     font-family: 'Lato';
     font-weight: bold;
     letter-spacing: 1px;
     text-transform: uppercase;
 }
 
 ul.tag-label li.tag-new {
     width: 66px;
     background-color: #000000;
     color: white;
     text-align: center;
     padding: 8px;
     position: absolute;
     top: 18px;
     left: 9px;
     transform: rotate(-90deg);
     font-family: 'Lato';
     font-size: 12px;
     height: 30px;
     letter-spacing: 3px;
     text-transform: uppercase;
 }
 
 ul.tag-label li.tag-new:after {
     content: "";
