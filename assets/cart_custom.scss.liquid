th {
  font-size: 16px;
  font-weight: normal;
  color:#1B1B1B;
}

.responsive-table td, th {
  border-bottom: 1px solid #ededed;
  font-family: "Lato";
}

textarea#CartSpecialInstructions, label[for='CartSpecialInstructions'] {
  display: none !important;
}

td {
  
  
  nav.size {
    li {
      width: 120px;
    }
    
    li.selected {
      background: #f7f7f7;
    }
    
    li.active div.value {
      
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }
    
    ul ul li {
      color: #6f6f6f;
      padding: 10px;
      overflow: hidden;      
      text-overflow: ellipsis;
    }
  }

  nav.color {
    li {
      width: 100px;
    }

    div.value {
      width: 25px;
      height: 25px;      
      border-radius: 50%;
      padding: 0px;
      
    }
    
    li div {
      border-radius: 10px ;
    }
    
  }
  
  nav {
    
    &> ul > li > div:not(.value) {
      height: 33px !important;
    }
    
    li {
      display: block;
      list-style-type: none;
      position: relative;
      background: white;
    }
    
    li.active {
      height: 42px ;      
      display: flex;
      justify-content: left;
      align-items: center;
    }
    
    &.empty li {
      background: #f7f7f7;
    }   
    
    
    li {
      ul {
         display: none;
         position: absolute;
         width: 95px;
         left: 0;
         top: 100%;
         margin: 0;
         padding: 0;
         z-index: 10 !important;
      }

      ul {
        li {
          cursor: pointer;
          padding: 8px 0;
          margin-bottom: 0px !important;
/*           border: 1px solid #c0c0c024; */
          border: 1px solid #e0e0e024;
          border-bottom: none;
          border-left-color: #e8e8e8 !important;
          border-right-color: #e8e8e8 !important
        }

        div:not(.value) {
          width: 50px;
          height: 20px;
          border: 1px solid silver;
          margin: 0px auto;
          border-radius:10px;
        }
        
        li:hover {
          background: #f7f7f7 !important;
        }
      }
    }
    
    &:not(.empty) li > ul {      
      width: calc(100% + 2px);
      margin-left: -1px;
      max-height: 250px;
      overflow: auto;
      border-bottom: 1px solid #e8e8e8;
    }
    
    div.value {
      
      margin: 0px !important;
      color: #28292c !important;
      text-align: center;
      display: block;
      text-align: left;
      font-size: 14px;
      font-family: Lato;
      font-weight: 500;
    }
  }
  
  
  
  ul::-webkit-scrollbar {
    width: 7px;
    height: 7px;
  }
  
  ul::-webkit-scrollbar-track {
    background: #f1f1f1; 
  }
  
  ul::-webkit-scrollbar-thumb {
    background: #888; 
  }
  
  ul::-webkit-scrollbar-thumb:hover {
    background: #555; 
  }
  
  nav {
    padding: 0px !important;
  }
  
  &> {
    
    nav {
      margin-top: 3px;
    }
    
    a .item-close svg {
      width: 25px;
      fill: #d4d4d4;
      margin:0;
    }
    
    a {
      font-size: 16px;
      color: #28292c;
      font-family: "Lato";
    }
    
    div.number {
      
      border: 1px solid #f2f2f2;
      width: 115px;
      height: 42px;
      padding-top: 12px;
      text-align: center;

      span {
        opacity: 1 !important;
      }

      span.num svg {
        width: 12px;
        fill: #000;
      }
      
      span.num.minus.min svg {
        fill: #d4d4d4 !important;
      }
      
      span.number {
        color: #000;
        display: inline-block;
        transform: translateY(-9px);
      }
    }
    
    div.promo {
      float: left;
      
      &> input {
        padding: 10px 20px;
        background: silver;
      } 
      
      &> span.select {
        display: inline-block;
        margin: 0 20px !important;
        width: 20px;
        height: 20px;
        border-radius: 50%;
        background: #ffa8b5;
        
        &[selected='selected'] > div {
          width: 10px;
          height: 6px;
          border-left: 2px solid white;
          border-bottom: 2px solid white;
          transform: rotate(-50deg);
          margin-left: 5px;
          margin-top: 5px;
        }
        
      }
    }
    
  }
  
  
  
}
.con_qual>{
  div.number {
      
      border: 1px solid #f2f2f2;
      width: 105px;
      height: 42px;
      padding-top: 12px;
      text-align: center;

      span {
        opacity: 1 !important;
      }

      span.num svg {
        width: 12px;
        fill: #000;
      }
      
      span.num.minus.min svg {
        fill: #d4d4d4 !important;
      }
      
      span.number {
        color: #000;
        display: inline-block;
        transform: translateY(-9px);
      }
    }
}

